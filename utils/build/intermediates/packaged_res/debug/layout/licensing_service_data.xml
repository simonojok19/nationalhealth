<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:orientation="vertical" >

	<TextView
		android:id="@+id/text_view_licensing_service_trial"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginTop="3pt"
		android:background="@drawable/back"
		android:text="@string/msg_trial"
		android:textColor="@android:color/black" />

	<TextView
		android:id="@+id/text_view_licensing_service_conf"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginTop="3pt"
		android:background="@drawable/back"
		android:text="@string/msg_configuration"
		android:textColor="@android:color/black" />

	<TextView
		android:id="@+id/text_view_licensing_service_log"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginTop="3pt"
		android:background="@drawable/back"
		android:text="@string/msg_log"
		android:textColor="@android:color/black" />

</LinearLayout>